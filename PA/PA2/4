#include <stdio.h>
#define BOARD_COLS 15
#define BOARD_ROWS 15

char board[BOARD_ROWS][BOARD_COLS];

int
main(void)
{
  char CRLF;
  for (int i = 0; i < BOARD_ROWS; ++i)
  {
    for (int j = 0; j < BOARD_COLS; ++j)
    {
      scanf("%c", &board[i][j]);
    }
    scanf("%c", &CRLF);
  }
  return 0;
}
